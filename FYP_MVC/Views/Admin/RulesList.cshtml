@model IEnumerable<FYP_MVC.Models.DAO.userFeedBack>

@{
    ViewBag.Title = "RulesList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>RulesList</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.numOfDim)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.intention)
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.numOfDim)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.intention)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditRulesList", new { id=item.ID }) |
            @Html.ActionLink("Details", "DetailsRulesList", new { id=item.ID }) |
            @Html.ActionLink("Delete", "DeleteRulesList", new { id=item.ID })
        </td>
    </tr>
}

</table>
